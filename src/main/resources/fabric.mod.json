{
  "schemaVersion": 1,
  "id": "sensorywizards",
  "version": "${version}",
  "name": "Sensory Wizards",
  "description": "An mod in testing about wizardry with wands!",
  "authors": [
    "ProfPlantBoy"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/sensorywizards/icon.png",
  "environment": "*",
