    "fabric-datagen": [
      "net.profplantboy.sensorywizards.SensoryWizardsDataGenerator"
    ],
    "client": [
      "net.profplantboy.sensorywizards.client.SensoryWizardsClient"
    ],
    "cardinal-components": [
